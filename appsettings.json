      {
        "Name": "Seq",
        "Args": { "serverUrl":  "http://localhost:5341" }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SeasideDbConnectionString": "Server=localhost;Database=seaside;User Id=SA;Password=@Password123;MultipleActiveResultSets=true;TrustServerCertificate=True"
  }
